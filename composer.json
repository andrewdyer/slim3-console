{
    "name": "andrewdyer/slim3-console",
    "description": "Command line interface for your Slim Framework application.",
    "keywords": ["php", "php7", "slim", "slim-3", "slim-framework", "console", "command"],
    "homepage": "https://github.com/andrewdyer/slim3-console",
    "authors": [
        {
            "name": "Andrew Dyer",
            "homepage": "https://andrewdyer.rocks",
            "role": "developer"
        }
    ],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Anddye\\Console\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anddye\\Console\\Tests\\": "tests"
        }
    },
    "require": {
        "slim/slim": "^3.12",
        "symfony/console": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "scripts": {
        "cs": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "check": [
            "composer cs"
        ]
    }
}
